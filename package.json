{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:tailwind": "concurrently \"next dev\" \"npm run tailwind -- --watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -i src/styles/globals.css -o src/styles/dist.css",
    "format": "prettier --write \"src/**/*.{js,ts,tsx,md}\"",
    "setup": "yarn && yarn dlx @yarnpkg/sdks vscode",
    "postinstall": "yarn tailwind"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@tanstack/react-query": "4.23.0",
    "@types/follow-redirects": "^1.14.1",
    "axios": "1.2.5",
    "clsx": "1.2.1",
    "concurrently": "7.5.0",
    "follow-redirects": "^1.15.2",
    "next": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.0",
    "recoil": "0.7.6",
    "styled-components": "6.0.0-beta.2",
    "styled-jsx": "5.1.0",
    "use-count-up": "3.0.1",
    "validator": "13.7.0"
  },
  "devDependencies": {
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "autoprefixer": "^10.4.7",
    "eslint": "8.19.0",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "latest",
    "postcss": "^8.4.14",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.6",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.3.1"
}
